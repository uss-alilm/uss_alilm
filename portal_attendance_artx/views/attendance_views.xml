<odoo>
    <data>
        <!-- Start Original View check in & out &  create portal user from employee -->
        <template id="portal_my_home_attendance" name="Portal Attendance" inherit_id="portal.portal_my_home" priority="20">
            <xpath expr="//div[@class='oe_structure']" position="inside">
                <div class="container text-center mt-3">
                    <button id="attendanceBtn" class="btn btn-success attendanceBtn" onclick="handleAttendance()">
                        <i class="fa fa-sign-in" aria-hidden="true"></i>
                        <span id="btnText">سجل الدخول</span>
                    </button>
                    <button id="checkoutBtn" class="btn btn-danger d-none mt-2" onclick="handleCheckout()">
                        <i class="fa fa-sign-out" aria-hidden="true"></i>
                        <span>تسجيل الخروج</span>
                    </button>
                </div>
            </xpath>
        </template>
        
        <template id="portal_my_home_sale" name="Show Quotations / Sales Orders" customize_show="True"
                  inherit_id="portal.portal_my_home" priority="20">
            <div class="oe_structure" position="inside">
                <div class="container">
                    <button id="attendanceBtn" class="btn btn-success attendanceBtn">
                        <i class="fa fa-sign-in" aria-hidden="true"></i>
                        <span id="btnText">سجل الدخول</span>
                    </button>
                </div>
            </div>
        </template>

        <record id="action_hr_employee_create_portal_user" model="ir.actions.server">
            <field name="name">Create Portal User</field>
            <field name="model_id" ref="hr.model_hr_employee"/>
            <field name="binding_model_id" ref="hr.model_hr_employee"/>
            <field name="binding_view_types">form</field>
            <field name="groups_id" eval="[(4, ref('base.group_erp_manager'))]"/>
            <field name="state">code</field>
            <field name="code">
                action = records.action_create_portal_user()
            </field>
        </record>
        <!-- End  Original View check in & out &  create portal user from employee -->

        <!-- Portal request correct attendance -->
        <!-- Button to reques tcorrect attendance -->
        <record id="view_hr_attendance_correction_form" model="ir.ui.view">
            <field name="name">hr.attendance.correction.form</field>
            <field name="model">hr.attendance.correction</field>
            <field name="arch" type="xml">
                <form string="Attendance Correction Request">
                    <sheet>
                        <group>
                            <field name="employee_id"/>
                            <field name="attendance_id"/>
                            <field name="original_check_in"/>
                            <field name="original_check_out"/>
                            <field name="corrected_check_in"/>
                            <field name="corrected_check_out"/>
                            <field name="reason"/>
                            <field name="state" readonly="1"/>
                        </group>
                        <footer>
                            <button name="action_approve" type="object" class="oe_highlight" string="Approve"
                                invisible="state != 'submitted'"/>
                            <button name="action_reject" type="object" class="btn btn-danger" string="Reject"
                                invisible="state != 'submitted'"/>
                        </footer>
                    </sheet>
                </form>
            </field>
        </record>
        <!-- End  request to Correct attendance -->
        <template id="portal_my_attendance_correction" name="Attendance Correction Requests">
            <t t-call="portal.portal_layout">
                <div class="container">
                    <h2>طلبات تصحيح الحضور</h2>
                        <!-- <table class="table"> -->
                        <table class="table table-striped">

                            <thead>
                            <tr>
                            <th>الدخول</th>
                            <th>الخروج</th>
                            <th>السبب</th>
                            <th>الحالة</th>
                            </tr>
                            </thead>
                            <tbody>
                            <t t-foreach="correction_requests" t-as="request">
                            <tr>
                            <td><t t-esc="request.check_in"/></td>
                            <td><t t-esc="request.check_out"/></td>
                            <td><t t-esc="request.note"/></td>
                            <td><t t-esc="request.state"/></td>
                            </tr>
                            </t>
                            </tbody>
                        </table>
            <h3>طلب تصحيح الحضور</h3>
                <form action="/portal/request_attendance_correction" method="post">
                    <!-- CSRF Token Required for Secure Submission -->
                    <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
                    <label>الحضور :</label>
                    <input type="datetime-local" name="check_in" required="true" class="form-control"/>
                    <label>الخروج :</label>
                    <input type="datetime-local" name="check_out" required="true" class="form-control"/>
                    <label>السبب:</label>
                    <textarea name="note" class="form-control"></textarea>
                    <br/>
                    <button type="submit" class="btn btn-primary">إرسال الطلب  </button>
                </form>
            </div>
            </t>
        </template>
        <record id="action_portal_attendance" model="ir.actions.server">
            <field name="name">My Attendance</field>
            <field name="model_id" ref="hr.model_hr_employee"/>
            <field name="binding_model_id" ref="hr.model_hr_employee"/>
            <field name="state">code</field>
            <field name="code">
                action = {
                "type": "ir.actions.act_url",
                "url": "/my/attendance",
                "target": "self",
                }
            </field>
        </record>
        <template id="portal_my_home_attendance"
            name="Portal Home Attendance "
            inherit_id="portal.portal_breadcrumbs" priority="2">
            <xpath expr="//ol[hasclass('o_portal_submenu')]" position="inside">
                <!-- <xpath expr="//div[@id='content']" position="inside"> -->
                <a href="/my/attendance" class="btn btn-primary">حضوري </a>
            </xpath>
        </template>
        <!-- <record id="portal_attendance_main_menu" model="ir.ui.menu">
            <field name="name">Attendance</field>
            <field name="parent_id" ref="portal.menu_my_home"/>
            <field name="sequence" eval="10"/>
            </record> -->
        <!-- <record id="portal_attendance_menu" model="ir.ui.menu">
            <field name="name">My Attendance</field>
            <field name="parent_id" ref="portal_attendance_main_menu"/>
            <field name="action" ref="action_portal_attendance"/>
            <field name="sequence" eval="20"/>
            </record> -->
        <!-- POrtal Tree ttee attendance -->
        <template id="portal_my_home_attendance" inherit_id="portal.portal_my_home" priority="10">
            <!--     <xpath expr="//div[@class='o_portal_docs list-group']" position="after"> -->
            <xpath expr="//div[@id='portal_client_category']" position="inside">
                <!-- <div class="list-group" style="background-color:green"> -->
                <div class="o_portal_index_card col-md-6" >
                    <a class="list-group-item list-group-item-action d-flex align-items-center justify-content-between"
                        href="/my/attendance" title="Attendance">
                        <img width="100px" src="/portal_attendance_artx/static/src/images/attendance-icon.svg" loading="lazy"/>
                        الحضور
                        <span class="badge text-bg-secondary rounded-pill">
                        <i class="fa fa-sign-in"></i>
                        </span>
                        <div>
                            <h5 class="mt-0 mb-1"> سجلات الحضور </h5>
                            <!-- <p class="m-0 text-600">  تتبع الحضور و الانصراف و التاخير و الخصومات من هنا   </p> -->
                        </div>
                    </a>
                </div>
            </xpath>
        </template>
        <template id="portal_my_attendance" name="Portal Attendance">
            <t t-call="portal.portal_layout">
                <div class="container">
                    <h3>سجلات الحضور و الانصراف</h3>
                            <table class="table table-striped">
                    <!-- <table class="table table-bordered"> -->
                        <thead>
                            <tr>
                                <!-- <th>Date</th> -->
                                <th>الحضور</th>
                                <th>الانصراف</th>
                                <th>ساعات العمل</th>
                                <th>الوقت الاضافي</th>
                                <th>التاخير</th>
                                <th>الخصم</th>
                                <th>تصحيح</th>
                            </tr>
                        </thead>
                        <tbody>
                            <t t-foreach="attendance_records" t-as="record">
                                <tr>
                                    <td>
                                        <t t-esc="record.get('check_in', '')"/>
                                    </td>
                                    <td>
                                        <t t-esc="record.get('check_out', '')"/>
                                    </td>
                                    <!-- <td><t t-if="'worked_hours' in record"><t t-esc="record.get('worked_hours', '')"/></t></td> -->
                                    <td>
                                        <t t-if="'worked_hours' in record">
                                            <t t-set="total_minutes" t-value="int(record['worked_hours'] * 60)"/>
                                            <t t-set="hours" t-value="total_minutes // 60"/>
                                            <t t-set="minutes" t-value="total_minutes % 60"/>
                                            <t t-esc="('%02d:%02d' % (hours, minutes))"/>
                                        </t>
                                    </td>
                                    <td>
                                        <t t-esc="record.get('overtime_minutes', '')"/>
                                    </td>
                                    <td>
                                        <t t-esc="record.get('lateness', '')"/>
                                    </td>
                                    <td>
                                        <t t-esc="record.get('deduction_amount', '')"/>
                                    </td>
                                    <td>
                                        <button type="button" class="btn btn-warning" data-bs-toggle="modal" t-att-data-bs-target="'#correctAttendanceModal-%s' % record['id']">
                                        تصحيح
                                        </button>
                                        <!-- Modal (Popup Form) -->
                                        <div class="modal fade" t-att-id="'correctAttendanceModal-%s' % record['id']" tabindex="-1" aria-hidden="true">
                                            <div class="modal-dialog">
                                                <div class="modal-content">
                                                    <div class="modal-header">
                                                        <h5 class="modal-title">تصحيح الحضور</h5>
                                                        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                                    </div>
                                                    <form action="/portal/request_attendance_correction" method="post">
                                                        <div class="modal-body">
                                                            <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
                                                            <input type="hidden" name="attendance_id" t-att-value="record['id']"/>
                                                            <label>الحضور (المسجل):</label>
                                                            <input type="datetime-local" name="original_check_in" t-att-value="record.get('check_in', '')" class="form-control" readonly="1"/>
                                                            <label>الخروج (المسجل):</label>
                                                            <input type="datetime-local" name="original_check_out" t-att-value="record.get('check_out', '')" class="form-control" readonly="1"/>
                                                            <label>تصحيح الحضور:</label>
                                                            <input type="datetime-local" name="corrected_check_in" class="form-control" required="true"/>
                                                            <label>تصحيح الانصراف:</label>
                                                            <input type="datetime-local" name="corrected_check_out" class="form-control" required="true"/>
                                                            <label>سبب التصحيح:</label>
                                                            <textarea name="correction_reason" class="form-control"></textarea>
                                                        </div>
                                                        <div class="modal-footer">
                                                            <button type="submit" class="btn btn-success">إرسال التصحيح</button>
                                                            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">إلغاء</button>
                                                        </div>
                                                    </form>
                                                </div>
                                            </div>
                                        </div>
                                    </td>
                                </tr>
                            </t>
                        </tbody>
                    </table>
                </div>
                <div class="container">
                    <h2>طلبات تصحيح الحضور</h2>
                        <table class="table table-striped">
                        <thead>
                            <tr>
                                <th>الدخول</th>
                                <th>الخروج</th>
                                <th>السبب</th>
                                <th>الحالة</th>
                            </tr>
                        </thead>
                        <tbody>
                            <t t-foreach="correction_requests" t-as="request">
                                <tr>
                                    <td>
                                        <t t-esc="request.check_in"/>
                                    </td>
                                    <td>
                                        <t t-esc="request.check_out"/>
                                    </td>
                                    <td>
                                        <t t-esc="request.note"/>
                                    </td>
                                    <td>
                                        <t t-esc="request.state"/>
                                    </td>
                                </tr>
                            </t>
                        </tbody>
                    </table>
                    <h3>طلب تصحيح الحضور</h3>
                    <form action="/portal/request_attendance_correction" method="post">
                        <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
                        <label>الحضور :</label>
                        <input type="datetime-local" name="check_in" required="true" class="form-control"/>
                        <label>الخروج :</label>
                        <input type="datetime-local" name="check_out" required="true" class="form-control"/>
                        <label>السبب:</label>
                        <textarea name="note" class="form-control"></textarea>
                        <br/>
                        <button type="submit" class="btn btn-primary">إرسال الطلب  </button>
                    </form>
                </div>
            </t>
        </template>
        <template id="portal_my_home_vendor_rfq" name="Show Vendor RFQs"
            customize_show="True" inherit_id="portal.portal_my_home"
            priority="25">
            <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
                <t t-call="portal.portal_docs_entry">
                    <t t-set="title">RFQ's</t>
                    <t t-set="url" t-value="'/my/vendor_rfqs'"/>
                    <t t-set="placeholder_count" t-value="'my_rfq_count'"/>
                </t>
            </xpath>
        </template>
        <!-- POrtal Tree ttee attendance  template -->
        <template id="portal_attendance_list" name="Portal Attendance Tree">
            <t t-call="portal.layout">
                <div class="container">
                    <h3>سجل الحضور و الانصراف</h3>
                    <t t-foreach="attendance_records" t-as="record">
                        <div>
                            <span>
                                <t t-esc="record.check_in"/>
                            </span>
                            <span>
                                <t t-esc="record.check_out"/>
                            </span>
                        </div>
                    </t>
                </div>
            </t>
        </template>
        <!-- POrtal Tree ttee attendance template -->
        <record id="action_hr_employee_create_portal_user" model="ir.actions.server">
            <field name="name">إنشاء مستخدم بوابة</field>
            <field name="model_id" ref="hr.model_hr_employee"/>
            <field name="binding_model_id" ref="hr.model_hr_employee"/>
            <field name="binding_view_types">form</field>
            <field name="groups_id" eval="[(4, ref('base.group_erp_manager'))]"/>
            <field name="state">code</field>
            <field name="code">
                action = records.action_create_portal_user()
            </field>
        </record>
        <!-- Leaves -->
        <!-- Action to Handle Leave Requests -->
        <record id="action_portal_request_leave" model="ir.actions.server">
            <!-- <field name="name">طلب إجازة</field> -->
            <field name="name">Request Leave</field>
            <field name="model_id" ref="hr.model_hr_employee"/>
            <field name="state">code</field>
            <field name="code">
                action = {
                "type": "ir.actions.act_url",
                "url": "/my/leaves",
                "target": "self",
                }
            </field>
        </record>
        <!-- Add "Request Leave" Card to Portal -->
        <template id="portal_my_home_leaves" inherit_id="portal.portal_my_home" priority="11">
            <xpath expr="//div[@id='portal_client_category']" position="inside">
                <div class="o_portal_index_card col-md-6" >
                    <a href="/my/leaves" title="Leaves" class="d-flex justify-content-start gap-2 align-items-center py-3 px-3 rounded text-decoration-none text-reset text-bg-light text-bg-danger">
                        <div class="o_portal_icon">
                            <img width="100px" src="/portal_attendance_artx/static/src/images/sun-bath-svgrepo-com.svg" loading="lazy"/>
                        </div>
                        <div>
                            <h5 class="mt-0 mb-1">طلبات الاجازة </h5>
                            <p class="m-0 text-600">اطلب و تتبع اجازاتك من هنا </p>
                        </div>
                    </a>
                </div>
            </xpath>
        </template>
        <template id="portal_my_leaves" name="Portal Leave List">
            <t t-call="portal.portal_layout">
                <div class="container">
                    <div class="d-flex justify-content-between align-items-center mb-3">
                        <h3>طلبات الاجازة</h3>
                        <!-- Button to Create New Leave Request -->
                        <h3>
                            <a href="/my/leave/new" class="btn btn-primary">
                            <i class="fa fa-plus me-1"></i>إنشاء طلب اجازة
                            </a>
                        </h3>
                    </div>
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th>نوع الاجازة</th>
                                <th>تاريخ البدء</th>
                                <th>تاريخ الانتهاء</th>
                                <th>الحالة</th>
                            </tr>
                        </thead>
                        <tbody>
                            <t t-foreach="leave_records" t-as="leave">
                                <tr>
                                    <!-- Display the name of the leave type -->
                                    <td>
                                        <t t-esc="leave.holiday_status_id.name"/>
                                    </td>
                                    <td>
                                        <t t-esc="leave.date_from"/>
                                    </td>
                                    <td>
                                        <t t-esc="leave.date_to"/>
                                    </td>
                                    <td>
                                        <t t-if="leave.state == 'draft'">مسودة</t>
                                        <t t-if="leave.state == 'confirm'">في انتظار الموافقة</t>
                                    </td>
                                </tr>
                            </t>
                        </tbody>
                    </table>
                </div>
            </t>
        </template>

        <template id="leave_form_template" name="Leave Form">
            <t t-call="portal.portal_layout">
                <section class="leave-form-section py-4">
                    <div class="container">
                        <div class="row justify-content-center">
                            <div class="col-md-8">
                                <div class="card shadow-sm">
                                    <div class="card-header bg-primary text-white">
                                        <h4 class="mb-0 text-center">طلب إجازة</h4>
                                    </div>
                                    <div class="card-body">
                                        <form action="/my/leave/submit" method="POST" id="leaveRequestForm">
                                            <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>

                                            <div class="mb-3">
                                                <label for="leave_type" class="form-label fw-bold">نوع الإجازة</label>
                                                <select id="leave_type" name="leave_type" class="form-select" required="true">
                                                    <option value="" disabled="true" selected="true">اختر نوع الإجازة</option>
                                                    <t t-foreach="leave_types" t-as="leave_type">
                                                        <option t-att-value="leave_type.id">
                                                            <t t-esc="leave_type.name"/>
                                                        </option>
                                                    </t>
                                                </select>
                                            </div>

                                            <div class="mb-3">
                                                <label for="start_date" class="form-label fw-bold">تاريخ البدء</label>
                                                <input type="date" id="start_date" name="start_date" class="form-control" required="true" />
                                            </div>

                                            <div class="mb-3">
                                                <label for="end_date" class="form-label fw-bold">تاريخ الانتهاء</label>
                                                <input type="date" id="end_date" name="end_date" class="form-control" required="true" />
                                            </div>

                                            <div id="error-message" class="text-danger fw-bold mb-3" style="display: none;"></div>

                                            <div class="d-grid">
                                                <button type="submit" class="btn btn-success">
                                                    <span id="submit-text">إرسال الطلب</span>
                                                    <span id="loading-spinner" class="spinner-border spinner-border-sm ms-2" role="status" aria-hidden="true" style="display: none;"></span>
                                                </button>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>

                <script>
                    document.getElementById('leaveRequestForm').addEventListener('submit', function(event) {
                        var startDate = document.getElementById('start_date').value;
                        var endDate = document.getElementById('end_date').value;
                        var errorMessage = document.getElementById('error-message');

                        if (startDate &amp;&amp; endDate &amp;&amp; new Date(startDate) > new Date(endDate)) {

                            event.preventDefault();
                            errorMessage.innerText = "يجب أن يكون تاريخ البدء قبل تاريخ الانتهاء.";
                            errorMessage.style.display = "block";
                            return;
                        } else {
                            errorMessage.style.display = "none";
                        }

                        document.getElementById('submit-text').innerText = "جارٍ الإرسال...";
                        document.getElementById('loading-spinner').style.display = "inline-block";
                    });
                </script>
            </t>
        </template>


